.pagination-wrap
  .pagination
    a.pagination__prev(href="#")
      +icon('chevron-left', 'pagination__icon')
    ul.pagination__list
      li.pagination__item
        a.pagination__link(href="#") 1
      li.pagination__item
        a.pagination__link(href="#") 2
      li.pagination__item
        a.pagination__link(href="#") 3
      li.pagination__item
        a.pagination__link.active(href="#") 4
      li.pagination__item &hellip;
      li.pagination__item
        a.pagination__link(href="#") 12
      li.pagination__item
        a.pagination__link(href="#") 13
      li.pagination__item
        a.pagination__link(href="#") 14
      li.pagination__item
        a.pagination__link(href="#") 15
    a.pagination__next(href="#")
      +icon('chevron-right', 'pagination__icon')